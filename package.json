{
  "name": "extracao-assectra",
  "version": "1.0.0",
  "description": "Projeto para extrair dados do site Assectra com Node.js, Puppeteer, Babel e Docker.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "start": "node dist/index.js",
    "employees:profiles": "node dist/index.js employees:profiles",
    "employees:documents": "node dist/index.js employees:documents",
    "companies:profiles": "node dist/index.js companies:profiles",
    "companies:documents": "node dist/index.js companies:documents",
    "dev": "nodemon -L --exec babel-node src/index.js",
    "dev:employees:profiles": "nodemon -L --exec babel-node src/index.js employees:profiles",
    "dev:employees:documents": "nodemon -L --exec babel-node src/index.js employees:documents",
    "dev:companies:profiles": "nodemon -L --exec babel-node src/index.js companies:profiles",
    "dev:companies:documents": "nodemon -L --exec babel-node src/index.js companies:documents"
  },
  "keywords": [
    "puppeteer",
    "web-scraping",
    "nodejs",
    "babel",
    "docker"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "puppeteer": "^19.7.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.0",
    "nodemon": "^3.1.10"
  }
}
